#!/usr/bin/env bash

OPTIND=1 # Reset getopts iterator

while getopts "ht:f:" opt; do
  case "$opt" in
  h|\?)
    echo "h or nothing?"
    exit 0
    ;;
  t) to_format=$OPTARG
    echo "set the to param to $OPTARG"
    ;;
  f) from_format=$OPTARG
    echo "set the from param to $OPTARG"
    ;;
  esac
done

echo "ok, byebye"
